{
  "name": "vite2-vue3-tsbranch",
  "version": "1.0.0",
  "license": "ISC",
  "author": "imbacc",
  "scripts": {
    "dev": "vite --mode development --host",
    "dev:test": "vite --mode testing --host",
    "dev:prod": "vite --mode production --host",
    "build": "vite build --mode production",
    "build:test": "vite build --mode testing",
    "build:prod": "vite build --mode production",
    "preview:test": "vite --mode testing preview --host",
    "preview:prod": "vite --mode production preview --host",
    "test:gzip": "npx http-server dist --cors --gzip -c-1",
    "force": "vite --mode development --host --force",
    "debug": "vite --mode development --host --debug",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.vue",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.vue",
    "czc": "git add . && cz",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "update:list": "ncu",
    "update:up": "ncu -u"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dayjs": "^1.11.10",
    "imba-cache": "^1.1.13",
    "imba-request": "^1.0.25",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.7",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@commitlint/config-conventional": "^18.1.0",
    "@types/lodash-es": "^4.17.10",
    "@unocss/preset-uno": "^0.57.2",
    "@unocss/vite": "^0.57.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "change-case": "^4.1.2",
    "commitizen": "^4.3.0",
    "commitlint": "^18.2.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.53.0",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "imba-config": "^1.0.8",
    "lint-staged": "^15.0.2",
    "mockjs": "^1.1.0",
    "rollup": "^4.3.0",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "typescript": "^5.2.2",
    "unocss": "^0.57.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-icons": "^0.17.3",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^3.0.0",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-prebundle": "^0.0.4",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imbacc/vite2-vue3.git"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
