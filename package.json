{
  "name": "vite2-vue3-tsbranch",
  "version": "1.0.0",
  "license": "ISC",
  "author": "imbacc",
  "scripts": {
    "dev": "vite --mode development --host",
    "dev:test": "vite --mode testing --host",
    "dev:prod": "vite --mode production --host",
    "build:test": "vite build --mode testing",
    "build:prod": "vite build --mode production",
    "preview:test": "vite --mode testing preview --host",
    "preview:prod": "vite --mode production preview --host",
    "test:gzip": "npx http-server dist --cors --gzip -c-1",
    "force": "vite --mode development --host --force",
    "debug": "vite --mode development --host --debug",
    "build": "vue-tsc --noEmit && vite build",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "czc": "git add . && pnpm exec cz",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "imba-cache": "^1.1.6",
    "imba-request": "^1.0.17",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.32",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.2",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/lodash-es": "^4.17.6",
    "@unocss/preset-uno": "^0.48.0",
    "@unocss/vite": "^0.48.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue-macros/reactivity-transform": "^0.2.4",
    "change-case": "^4.1.2",
    "commitizen": "^4.3.0",
    "commitlint": "^17.4.4",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.2",
    "imba-eslint-config": "^1.0.2",
    "lint-staged": "^13.1.0",
    "mockjs": "^1.1.0",
    "rollup": "^2.72.1",
    "sass": "^1.51.0",
    "stylelint": "^14.16.0",
    "typescript": "^4.5.4",
    "unocss": "^0.48.0",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-icons": "^0.15.0",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.1.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.28.0",
    "vitest": "^0.26.3",
    "vue-tsc": "^0.34.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imbacc/vite2-vue3.git"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
