{
	"name": "vite2-vue3-tsbranch",
	"version": "1.0.0",
	"license": "ISC",
	"author": "imbacc",
	"scripts": {
		"dev": "cross-env NODE_ENV=development vite --mode development --host",
		"dev:test": "cross-env NODE_ENV=testing vite --mode testing --host",
		"dev:prod": "cross-env NODE_ENV=production vite --mode production --host",
		"build:test": "cross-env NODE_ENV=testing vite build --mode testing",
		"build:prod": "cross-env NODE_ENV=production vite build --mode production",
		"preview:test": "cross-env NODE_ENV=testing vite --mode testing preview --host",
		"preview:prod": "cross-env NODE_ENV=production vite --mode production preview --host",
		"test:gzip": "npx http-server dist --cors --gzip -c-1",
		"force": "cross-env NODE_ENV=development vite --mode development --host --force",
		"debug": "cross-env NODE_ENV=development vite --mode development --host --debug",
		"build": "cross-env NODE_ENV=production vue-tsc --noEmit && vite build"
	},
	"dependencies": {
		"@vueuse/core": "^8.4.2",
		"axios": "^0.27.2",
		"imba-cache": "^1.1.0",
		"imba-lazy": "^1.0.8",
		"imba-request": "^1.0.14",
		"lodash": "^4.17.21",
		"mitt": "^3.0.0",
		"nprogress": "^0.2.0",
		"pinia": "^2.0.14",
		"vue": "^3.2.25",
		"vue-router": "^4.0.15"
	},
	"devDependencies": {
		"@types/node": "^17.0.34",
		"@vitejs/plugin-vue": "^2.3.1",
		"autoprefixer": "^10.4.7",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.0",
		"mockjs": "^1.1.0",
		"postcss": "^8.4.13",
		"postcss-pxtorem": "^6.0.0",
		"rollup": "^2.72.1",
		"rollup-plugin-gzip": "^3.0.1",
		"ts-node": "^10.7.0",
		"typescript": "^4.5.4",
		"unplugin-icons": "^0.14.3",
		"unplugin-vue-components": "^0.19.3",
		"vite": "^2.9.7",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-mock": "^2.9.6",
		"vite-plugin-pages": "^0.23.0",
		"vite-plugin-windicss": "^1.8.4",
		"vue-tsc": "^0.34.7"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/imbacc/vite2-vue3.git"
	}
}
